version: 1.1.4

cache: true

endpoints:
  azureOpenAI:
    titleConvo: true
    titleModel: "gpt-4o-mini"
    assistants: false # Agents are better
    plugins: true
    agents:
      recursionLimit: 50
      disableBuilder: false
    groups:
