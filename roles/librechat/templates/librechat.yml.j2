        apiKey: "{{ OPENAI_AZURE_API_KEY_1 }}"
        instanceName: "{{ OPENAI_AZURE_INSTANCENAME_1 }}"
        version: "{{ OPENAI_AZURE_API_VERSION }}"
        models:
          gpt-4o:
            deploymentName: "gpt-4o"
          gpt-4o-mini:
            deploymentName: "gpt-4o-mini"
      - group: "Sweden-Reasoning"
        apiKey: "009bc3df116048fb90aa6793e16bce47"
        instanceName: "saunachatse"
        version: "2024-12-01-preview"
        dropParams: ["stream", "temperature", "top_p", "presence_penalty", "frequency_penalty", "best_of", "max_tokens", "stop"]
        models:
          o3-mini:
            deploymentName: "o3-mini"
          o1:
            deploymentName: "o1"   
{# whisper:
  deploymentName: whisper-01 #}
      {# - group: "llama-70b-chat"
        apiKey: "{{ OPENAI_AZURE_LLAMA_API_KEY }}"
